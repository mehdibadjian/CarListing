target 'CarListing' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CarListing
  # UIs
  pod 'SnapKit', '~> 4.0.0'
  # Network and datas
  pod 'RealmSwift'

  target 'CarListingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CarListingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
